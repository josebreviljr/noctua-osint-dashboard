# Noctua OSINT Collector Configuration
# Copy this file to .env and fill in your actual values

# Required: Your Google API key for Gemini
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Gemini model to use (default: gemini-1.5-flash)
GEMINI_MODEL=gemini-1.5-flash

# Optional: Maximum articles to collect per feed (default: 5)
MAX_ARTICLES_PER_FEED=5

# Optional: Path to feeds file (default: feeds.txt)
FEEDS_FILE=feeds.txt

# Optional: Output file path (default: data/reports.json)
OUTPUT_FILE=data/reports.json

# Optional: Request timeout in seconds (default: 30)
REQUEST_TIMEOUT=30

# Optional: Maximum retries for failed requests (default: 3)
MAX_RETRIES=3 